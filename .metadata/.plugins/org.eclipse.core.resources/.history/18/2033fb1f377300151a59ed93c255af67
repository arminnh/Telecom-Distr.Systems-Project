package server;

/* 
 * It allows clients to invoke methods of remote objects by identifying 
 * the appropriate method in the remote object interface and passing the request message to the object
 */
public class Dispatcher {
	//SEE JAVA REFLECTION
	
	switch(message):
		case "methodeEen":
			break;
		case "methodeTwee":
			break;
		case default:
			break;
}
